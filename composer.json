{
    "name": "vinterskogen/laravel-uploaded-image",    
    "description": "Gracefully deal with resizing, cropping and scaling uploaded images in Laravel apps",
    "license": "MIT",
    "authors": [
        {
            "name": "Vinterskogen Skogen",
            "email": "vinter.skogen@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.0",
        "intervention/image": "^2.2",
        "illuminate/support": "~5.4",
        "illuminate/http": "~5.4",
        "symfony/http-foundation": "~3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "~0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Vinterskogen\\UploadedImage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinterskogen\\UploadedImage\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vinterskogen\\UploadedImage\\UploadedImageServiceProvider"
            ]
        }
    }
}
